{
  "files.associations": {
    "**/*.html": "html",
    "**/templates/**/*.html": "django-html",
    "**/templates/**/*": "django-txt",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "vector": "cpp"
  },

  "code-runner.executorMap": { "java": "cd $dir && javac $fileName -encoding utf-8 && java $fileNameWithoutExt" }

}